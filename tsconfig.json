{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "isolatedModules": true,
    "types": ["astro/client", "react", "react/jsx-runtime"],
    "outDir": "./dist",
    "moduleResolution": "node",
    "jsx": "react",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
    }
  }
}
